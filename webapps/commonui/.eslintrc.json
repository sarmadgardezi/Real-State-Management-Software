{
  "extends": ["plugin:react/recommended"],
  "plugins": ["react"],
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "import/no-named-as-default": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
